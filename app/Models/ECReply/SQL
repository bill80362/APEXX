CREATE TABLE `ECReply` (
  `ECReplyID` int(11) NOT NULL,
  `POST` int(11) NOT NULL,
  `created_at` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

--
-- 已傾印資料表的索引
--

--
-- 資料表索引 `ECReply`
--
ALTER TABLE `ECReply`
  ADD PRIMARY KEY (`ECReplyID`);

--
-- 在傾印的資料表使用自動遞增(AUTO_INCREMENT)
--

--
-- 使用資料表自動遞增(AUTO_INCREMENT) `ECReply`
--
ALTER TABLE `ECReply`
  MODIFY `ECReplyID` int(11) NOT NULL AUTO_INCREMENT;
COMMIT;
